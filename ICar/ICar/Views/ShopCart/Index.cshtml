@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.ShopCart.ListShopItems)
    {
        <div class="alert-warning" mt-3>
            <b>Автомобил:</b>@el.Car.Name<br />
            <b>Цена:</b>@el.Car.Price.ToString("c")

        </div>
    }
    <hr />
    <a class="btn btn-danger mb-5" asp-controller="Order" asp-action="CheckOut">Плащане</a>

</div>